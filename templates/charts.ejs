<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('partials/head', { title: 'Analytics' }) %>
    <link rel="stylesheet" href="/history.css">
    <link rel="stylesheet" href="/charts.css">
</head>
<body>
<main>
<%- include('partials/header', { title: 'Analytics' }) %>
<section>
    <% for (const chart of charts) { %>
        <div class="chart-container">
            <div class="buttons">
                <button>Week</button>
                <button>Month</button>
                <button>Year</button>
            </div>
            <div class="chart">
                <h2><%=chart.name%></h2>
                <img src="<%=chart.image%>" alt="Chart">
            </div>
        </div>
    <% } %>
</section>
</main>
</body>
</html>